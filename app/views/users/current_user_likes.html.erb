<div class="page-header">
  <h1>
    Current User Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <% @user.likes.each do |like| %>
      <tr>
        <td><%= like.photo_id %></td>
        <td><%= Photo.find(like.photo_id).caption %></td>

        <td><a href="/photos/<%= like.photo_id %>"><img src="<%= Photo.find(like.photo_id).image %>"> </a></td>
        <% end %>
      </dl>
    </div>
  </div>
