<div class="page-header">
  <h1>
    Current User Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <% @user.likes.each do |like| %>
      <tr>
        <td><%= like.photo_id %></td>

        <td><img src="<%= Photo.find_by(3).image %>"> </td>

      </tr>
      <% end %>

  </div>
</div>
